    "\n",
    "import websockets\n",
    "uri = \"ws://localhost:8000/ws/main\"\n",
    "async with websockets.connect(uri) as websocket:\n",
    "    while True:\n",
    "        response = await websocket.recv()\n",
    "        print(f\"Received: {response}\")"
